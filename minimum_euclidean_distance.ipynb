    "import math\n",
    "\n",
    "points = [(2,3), (4,5), (5,7)]\n",
    "\n",
    "def euclideanDistance(x,y):\n",
    "    return math.sqrt((x-y)**2 + (x+y)**2)\n",
    "    \n",
    "distances = []\n",
    "\n",
    "for i,j in points:\n",
    "    res =euclideanDistance(i,j)\n",
    "    distances.append(res)\n",
    "\n",
    "print(f\"Mininum Euclidean Distance: {min(distances)}\")\n",
    "    \n"
   ]
  }
